@model MvcKutuphane.Models.Entity.TBLPERSONEL
@{
    ViewBag.Title = "PersonelEkle";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Personel Ekle</h2>

<form class="form-group" method="post">

    <div style="margin-top:10px;">
        <label>Personel Adı:</label>
        @*<input style="width:25%;" type="text" class="form-control" name="PERSONEL" placeholder="Kategori adı giriniz..." />*@

        @Html.TextBoxFor(x=>x.PERSONEL, new {@class="form-control"})
        @Html.ValidationMessageFor(x=>x.PERSONEL, "", new { @style="color:red;"} )
    </div>


    <div style="margin-top:10px;">
        <button class="btn btn-info" >Personel Ekle</button>
    </div>

</form>
