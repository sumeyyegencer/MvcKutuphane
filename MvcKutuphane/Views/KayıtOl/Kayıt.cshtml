    <link href="~/Content/webregister/css/style.css" type="text/css" rel="stylesheet" media="all">
    <!-- //css files -->

    <link rel="stylesheet" href="~/Content/webregister/css/font-awesome.css"> <!-- Font-Awesome-Icons-CSS -->

</head>
<!-- body starts -->
<body>
    <!-- section -->
    <section class="register">
        <div class="register-full">
            <div class="register-left">
                <div class="register">
                    <div class="logo">
                        <a href="#"><span class="fa fa-graduation-cap" aria-hidden="true"></span></a>
                    </div>
                    <h1>MVC5 KÜTÜPHANE YÖNETİM SİSTEMİ</h1>
                    <p>
                        Hoşgeldiniz... Panele kayıt olmak için lütfen formu doldurunuz.  
                    </p>
                    <div class="content3">
                        <ul>
                            <li><a class="" href="/Login/GirisYap"> Giriş Yap</a></li>
                            <li><a class="read" href="/Vitrin/Index"> Siteye Dön</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="register-right">
                <div class="register-in">
                    <h2><span class="fa fa-pencil"></span> Kayıt Paneli</h2>
                    <div class="register-form">
                        <form action="#" method="post">
                            <div class="fields-grid">
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="First name" required="">
                                    <label>Adı:</label>
                                    <span></span>
                                </div>
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="Last name" required="">
                                    <label>Soyadı:</label>
                                    <span></span>
                                </div>
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="Nick name" required="">
                                    <label>Kullanıcı Adı:</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="email" name="Email" required="">
                                    <label>Email</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="tel" name="Telefon" required="">
                                    <label>Telefon</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="password" name="password" required="">
                                    <label>Şifre</label>
                                    <span></span>
                                </div>
                                <div class="styled-input agile-styled-input-top">
                                    <select id="category2" required="">
                                        <option value="">Okulunuz:</option>
                                        <option value="">Sakarya Üniversitesi</option>
                                        <option value="">Harran üniversitesi</option>
                                        <option value="">Ankara Üniversitesi</option>
                                        <option value="">Bilkent üniversitesi</option>
                                        <option value="">Uludağ Üniversitesi</option>
                                    </select>
                                    <span></span>
                                </div>
                                <div class="clear"> </div>
                                <label class="checkbox"><input type="checkbox" name="checkbox" checked><i></i>Şartları okudum<a href="#">kabul ediyorum.</a></label>
                            </div>
                            <input type="submit" value="KAYIT OL">
                        </form>
                    </div>
                </div>
                <div class="clear"> </div>
            </div>
            <div class="clear"> </div>
        </div>
        <!-- copyright -->
        <p class="agile-copyright">© 2022 Kütüphane Sistemi. Tüm hakları saklıdır. | Design by <a href="https://w3layouts.com/" target="_blank">W3layouts</a></p>
        <!-- //copyright -->
    </section>
    <!-- //section -->
</body>
<!-- //body ends -->
</html>
